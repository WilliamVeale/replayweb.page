{
  "platform": {
    "apiRuntime": "node:20"
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".wasm": "application/wasm",
    ".warc": "application/warc",
    ".warc.gz": "application/gzip",
    ".wacz": "application/zip",
    ".har": "application/json"
  },
  "globalHeaders": {
    "Cross-Origin-Embedder-Policy": "credentialless",
    "Cross-Origin-Opener-Policy": "same-origin"
  }
}